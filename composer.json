{
    "name": "bitmatrix/craft-omnisearch",
    "description": "Powerful search filters for the control panel with full Matrix support",
    "type": "craft-plugin",
    "version": "1.0.6",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "omnisearch",
        "search",
        "cp",
        "query"
    ],
    "support": {
        "docs": "https://github.com/bitmatrixstudio/craft-omnisearch/blob/master/README.md",
        "issues": "https://github.com/bitmatrixstudio/craft-omnisearch/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Bitmatrix Studio",
            "homepage": "https://github.com/bitmatrixstudio"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "bitmatrix\\omnisearch\\": "src/",
            "fixtures\\": "tests/fixtures/"
        }
    },
    "extra": {
        "name": "OmniSearch",
        "handle": "omnisearch",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/bitmatrix/craft-omnisearch/master/CHANGELOG.md",
        "components": {
            "omniSearchService": "bitmatrix\\omnisearch\\services\\OmniSearchService"
        },
        "class": "bitmatrix\\omnisearch\\OmniSearch"
    },
    "require-dev": {
        "codeception/codeception": "3.1.*",
        "vlucas/phpdotenv": "^3.0"
    }
}
